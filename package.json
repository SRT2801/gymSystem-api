{
  "name": "gymsystem-api",
  "version": "1.0.0",
  "description": "Sistema de gestión de membresías para gimnasio",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --watch src --ext ts,json --ignore src/**/*.spec.ts --exec ts-node -r tsconfig-paths/register src/index.ts",
    "test": "jest",
    "seed": "ts-node -r tsconfig-paths/register src/infrastructure/database/seeds/index.ts",
    "seed:memberships": "ts-node -r tsconfig-paths/register src/infrastructure/database/seeds/membershipSeeder.ts"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^6.14.2",
    "jsonwebtoken": "^9.0.0",
    "module": "^1.2.5",
    "mongoose": "^6.9.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.13.0",
    "jest": "^29.4.2",
    "nodemon": "^3.1.10",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.5"
  }
}
